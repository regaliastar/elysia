{
  "name": "elysia-rss",
  "displayName": "Elysia rss",
  "version": "0.0.1",
  "description": "Anime Chrome manages RSS plugins.",
  "author": "regaliastar",
  "packageManager": "npm@6.14.17",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.1",
    "@plasmohq/storage": "^1.5.0",
    "plasmo": "0.69.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.6.4",
    "@types/chrome": "0.0.227",
    "@types/node": "18.15.11",
    "@types/react": "18.0.32",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.39.0",
    "eslint-plugin-react": "^7.32.2",
    "prettier": "2.8.7",
    "typescript": "5.0.3"
  },
  "manifest": {
    "permissions": [
      "storage",
      "tabs"
    ],
    "host_permissions": [
      "https://*/*"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "tabs/index.html"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ],
    "content_scripts": [{
      "matches": ["https://*/*", "http://*/*"]
    }]
  }
}
