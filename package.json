{
  "name": "elysia-rss",
  "displayName": "Elysia-rss",
  "version": "0.0.1",
  "description": "Anime Chrome manages RSS plugins.",
  "author": "regaliastar",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack -w --mode=production --progress",
    "build": "webpack --mode=production --progress",
    "release": "npm run build && mkdir release && zip -r release/elysia.zip dist",
    "format": "eslint \"**/*.js\" --fix && prettier \"**/*.{ts,tsx,js,scss,less}\" --write",
    "format:staged": "eslint \"**/*.js\" --fix && pretty-quick --staged --verbose --pattern \"**/*.{ts,tsx,js,scss,less}\"",
    "format:check": "eslint \"**/*.js\" && prettier-check \"**/*.{ts,tsx,js,scss,less}\"",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --fix"
  },
  "dependencies": {
    "@arco-design/theme-line": "^0.0.3",
    "@arco-design/web-react": "^2.47.1",
    "@arco-plugins/webpack-react": "^1.4.9-beta.2",
    "buffer": "^6.0.3",
    "https-browserify": "^1.0.0",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "react": "^16.14.0",
    "react-dom": "18.2.0",
    "rss-parser": "^3.13.0",
    "stream": "^0.0.2",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "timers": "^0.1.1",
    "timers-browserify": "^2.0.12",
    "url": "^0.11.0",
    "webpack": "^5.80.0"
  },
  "devDependencies": {
    "@types/chrome": "0.0.227",
    "@types/node": "18.15.11",
    "@types/react": "^16.14.0",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.39.0",
    "eslint-plugin-react": "^7.32.2",
    "html-webpack-plugin": "^5.5.1",
    "less-loader": "^11.1.0",
    "mini-css-extract-plugin": "^2.7.5",
    "prettier": "2.8.7",
    "style-loader": "^3.3.2",
    "ts-loader": "^9.4.2",
    "typescript": "5.0.3",
    "url-loader": "^4.1.1",
    "webpack-cli": "^5.0.2",
    "husky": "^8.0.0"
  }
}
